{
  "expo": {
    "name": "AstroCalc",
    "slug": "astrocalc-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "astrocalc",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E1B4B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.astrocalc.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#1E1B4B"
      },
      "package": "com.astrocalc.mobile"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ],
    "updates": {
      "enabled": false,
      "checkAutomatically": "NEVER",
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "nativeVersion"
    },
    "experiments": {
      "typedRoutes": true
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}
